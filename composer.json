{
    "name": "monofony/skeleton",
    "type": "project",
    "description": "A Minimal Monofony project to create applications.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.7",
        "cocur/slugify": "^4.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "3.1.*",
        "doctrine/migrations": "3.2.*",
        "doctrine/orm": "^2.5",
        "fakerphp/faker": "^1.14",
        "gesdinet/jwt-refresh-token-bundle": "^1.0",
        "league/flysystem": "^3.2",
        "lexik/jwt-authentication-bundle": "^2.14",
        "liip/imagine-bundle": "^2.2",
        "monofony/admin": "^0.8",
        "monofony/admin-contracts": "^0.8",
        "monofony/admin-meta": "^0.8",
        "monofony/api-contracts": "^0.8",
        "monofony/api-meta": "^0.8",
        "monofony/core-bundle": "^0.8",
        "monofony/core-contracts": "^0.8",
        "monofony/core-meta": "^0.8",
        "monofony/sylius-user-bridge": "^0.8",
        "nelmio/cors-bundle": "^2.2",
        "pagerfanta/pagerfanta": "^3.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.5",
        "psr/event-dispatcher": "^1.0",
        "ramsey/uuid": "^4.3",
        "sonata-project/block-bundle": "^4.2",
        "sylius/grid-bundle": "^1.11",
        "sylius/mailer": "^1.5",
        "sylius/resource": "^1.8",
        "sylius/resource-bundle": "^1.9",
        "sylius/ui-bundle": "^1.11",
        "sylius/user": "^1.11",
        "sylius/user-bundle": "^1.11",
        "symfony/asset": "5.4.*",
        "symfony/config": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "2.1.*",
        "symfony/mailer": "5.4.*",
        "symfony/maker-bundle": "^1.45",
        "symfony/messenger": "5.4.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "6.1.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/routing": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/templating": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.9",
        "thecodingmachine/safe": "^2.2",
        "twig/twig": "^2.7",
        "vich/uploader-bundle": "^1.8",
        "webmozart/assert": "^1.9",
        "zenstruck/foundry": "^1.14"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/mink-selenium2-driver": "^1.3",
        "dbrekelmans/bdi": "^1.0",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "lchrusciel/api-test-case": "^5.1",
        "monofony/behat-bridge": "^0.8",
        "phpspec/phpspec": "^7.2",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-nette": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "phpstan/phpstan-webmozart-assert": "^1.1",
        "rector/rector": "0.13.0",
        "robertfausk/behat-panther-extension": "^1.1",
        "se/selenium-server-standalone": "2.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/web-profiler-bundle": "5.4.*",
        "symplify/config-transformer": "^10.3",
        "symplify/easy-coding-standard": "^10.2",
        "vimeo/psalm": "^4.22"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*",
            "endpoint": "flex://defaults"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
